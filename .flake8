[flake8]
max-line-length = 100
max-complexity = 10
exclude =
    .git,
    __pycache__,
    .pytest_cache,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    migrations/versions,
    migrations/*.py
    alembic
ignore =
    E203,
    W503,
    E712
per-file-ignores =
    __init__.py:F401
    migrations/versions/*.py:ALL
